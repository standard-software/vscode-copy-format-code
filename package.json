{
  "name": "vscode-copy-format-code",
  "displayName": "Copy Format Code",
  "description": "This extension can be copied with code headers and other decorations.",
  "version": "0.2.0",
  "icon": "vscode-copy-format-code_icon.png",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "homepage": "https://github.com/standard-software/vscode-copy-format-code",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-copy-format-code"
  },
  "bugs": {
    "url": "https://github.com/standard-software/vscode-copy-format-code/issues"
  },
  "engines": {
    "vscode": "^1.89.1"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "copy",
    "format",
    "code"
  ],
  "enabledApiProposals": [
    "quickPickSeparators"
  ],
  "scripts": {
    "lint": "npx eslint",
    "lintfix": "npx eslint --fix",
    "pack": "npx -y vsce package && code --install-extension vscode-copy-format-code-0.2.0.vsix",
    "": ""
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "eslint": "^9.3.0",
    "globals": "^15.3.0"
  },
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "CopyFormatCode",
      "properties": {
        "CopyFormatCode.CopyFormat": {
          "type": "array",
          "default": [
            {
              "label": "Markdown",
              "format": {
                "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName% : %LineNumberStart%-%LineNumberEnd%",
                "body": "%Line%",
                "bodySeparator": "",
                "footer": "```"
              }
            },
            {
              "label": "Markdown LineNumber",
              "format": {
                "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName%",
                "body": "%NumberFile%: %Line%",
                "bodySeparator": ":",
                "footer": "```"
              }
            },
            {
              "label": "Header RelativePath",
              "format": {
                "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                "body": "%Line%",
                "bodySeparator": ""
              }
            },
            {
              "label": "Header RelativePath LineNumber",
              "format": {
                "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                "body": "%NumberFile%: %Line%",
                "bodySeparator": ":"
              }
            },
            {
              "label": "Menu",
              "separator": true
            },
            {
              "label": "Test",
              "visible": false,
              "items": [
                {
                  "label": "Output Header",
                  "format": {
                    "header": "\\%FilePath\\%:  %FilePath%\n\\%FilePathSlash\\%:  %FilePathSlash%\n\\%FileName\\%:  %FileName%\n\\%FileNameWithoutExt\\%:  %FileNameWithoutExt%\n\\%FileExt\\%:  %FileExt%\n\n\\%FolderPath\\%:  %FolderPath%\n\\%FolderPathSlash\\%:  %FolderPathSlash%\n\\%FolderName\\%:  %FolderName%\n\n\\%FilePathRelative\\%:  %FilePathRelative%\n\\%FilePathRelativeSlash\\%:  %FilePathRelativeSlash%\n\\%FolderPathRelative\\%:  %FolderPathRelative%\n\\%FolderPathRelativeSlash\\%:  %FolderPathRelativeSlash%\n\n\\%FilePathRelativeProject\\%:  %FilePathRelativeProject%\n\\%FilePathRelativeProjectSlash\\%:  %FilePathRelativeProjectSlash%\n\\%FolderPathRelativeProject\\%:  %FolderPathRelativeProject%\n\\%FolderPathRelativeProjectSlash\\%:  %FolderPathRelativeProjectSlash%\n\n\\%ProjectFolderPath\\%:  %ProjectFolderPath%\n\\%ProjectFolderPathSlash\\%:  %ProjectFolderPathSlash%\n\\%ProjectName\\%:  %ProjectName%\n\n\\%LineNumberStart\\%:  %LineNumberStart%\n\\%LineNumberStartZeroPad\\%:  %LineNumberStartZeroPad%\n\\%LineNumberEnd\\%:  %LineNumberEnd%\n",
                    "body": "%NumberFile%: %Line%"
                  }
                },
                {
                  "label": "Output Body",
                  "format": {
                    "body": "%Line%"
                  }
                },
                {
                  "label": "Output Body LineNumberFile",
                  "format": {
                    "header": "",
                    "body": "%NumberFile%: %Line%",
                    "bodySeparator": "---"
                  }
                },
                {
                  "label": "Output Body LineNumberStart1",
                  "format": {
                    "header": "-----",
                    "body": "%NumberStart1%: %LineCutMinIndent%",
                    "footer": "---"
                  }
                },
                {
                  "label": "Output Body LineTrimFirst SpacePadEnd",
                  "format": {
                    "header": "",
                    "body": "%NumberFile%: %LineTrimFirst%%SpacePadEnd%",
                    "bodySeparator": ":"
                  }
                },
                {
                  "label": "Output Body LineTrimEnd SpacePadEnd SkipBlankLine",
                  "format": {
                    "header": "",
                    "body": "%SkipBlankLine%%NumberFile%: %LineTrimLast%%SpacePadEnd%",
                    "bodySeparator": ":"
                  }
                }
              ]
            },
            {
              "label": "Delete Indent and BlankLine",
              "items": [
                {
                  "label": "Delete BlankLine",
                  "items": [
                    {
                      "label": "Markdown",
                      "separator": true
                    },
                    {
                      "label": "Markdown",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName% : %LineNumberStart%-%LineNumberEnd%",
                        "body": "%SkipBlankLine%%Line%",
                        "bodySeparator": "",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Markdown LineNumber",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName%",
                        "body": "%SkipBlankLine%%NumberFile%: %Line%",
                        "bodySeparator": ":",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Text",
                      "separator": true
                    },
                    {
                      "label": "Header RelativePath",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%SkipBlankLine%%Line%",
                        "bodySeparator": ""
                      }
                    },
                    {
                      "label": "Header RelativePath LineNumber",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%SkipBlankLine%%NumberFile%: %Line%",
                        "bodySeparator": ":"
                      }
                    }
                  ]
                },
                {
                  "label": "Delete Indent",
                  "items": [
                    {
                      "label": "Markdown",
                      "separator": true
                    },
                    {
                      "label": "Markdown",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName% : %LineNumberStart%-%LineNumberEnd%",
                        "body": "%LineTrimFirst%",
                        "bodySeparator": "",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Markdown LineNumber",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName%",
                        "body": "%NumberFile%: %LineTrimFirst%",
                        "bodySeparator": ":",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Text",
                      "separator": true
                    },
                    {
                      "label": "Header RelativePath",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%LineTrimFirst%",
                        "bodySeparator": ""
                      }
                    },
                    {
                      "label": "Header RelativePath LineNumber",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%NumberFile%: %LineTrimFirst%",
                        "bodySeparator": ":"
                      }
                    }
                  ]
                },
                {
                  "label": "Cut Min Indent",
                  "items": [
                    {
                      "label": "Markdown",
                      "separator": true
                    },
                    {
                      "label": "Markdown",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName% : %LineNumberStart%-%LineNumberEnd%",
                        "body": "%LineCutMinIndent%",
                        "bodySeparator": "",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Markdown LineNumber",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName%",
                        "body": "%NumberFile%: %LineCutMinIndent%",
                        "bodySeparator": ":",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Text",
                      "separator": true
                    },
                    {
                      "label": "Header RelativePath",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%LineCutMinIndent%",
                        "bodySeparator": ""
                      }
                    },
                    {
                      "label": "Header RelativePath LineNumber",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%NumberFile%: %LineCutMinIndent%",
                        "bodySeparator": ":"
                      }
                    }
                  ]
                },
                {
                  "label": "Cut Min Indent and Delete BlankLine",
                  "items": [
                    {
                      "label": "Markdown",
                      "separator": true
                    },
                    {
                      "label": "Markdown",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName% : %LineNumberStart%-%LineNumberEnd%",
                        "body": "%SkipBlankLine%%LineCutMinIndent%",
                        "bodySeparator": "",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Markdown LineNumber",
                      "format": {
                        "header": "```%FileExt%\n// %FolderPathRelativeProjectSlash%/\n// %FileName%",
                        "body": "%SkipBlankLine%%NumberFile%: %LineCutMinIndent%",
                        "bodySeparator": ":",
                        "footer": "```"
                      }
                    },
                    {
                      "label": "Text",
                      "separator": true
                    },
                    {
                      "label": "Header RelativePath",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%SkipBlankLine%%LineCutMinIndent%",
                        "bodySeparator": ""
                      }
                    },
                    {
                      "label": "Header RelativePath LineNumber",
                      "format": {
                        "header": "%FolderPathRelativeProjectSlash%/\n%FileName%",
                        "body": "%SkipBlankLine%%NumberFile%: %LineCutMinIndent%",
                        "bodySeparator": ":"
                      }
                    }
                  ]
                }
              ]
            }
          ],
          "description": ""
        }
      }
    },
    "commands": [
      {
        "command": "vscode-copy-format-code.SelectFormat",
        "title": "Copy Format Code : Select Format..."
      }
    ],
    "keybindings": []
  }
}
